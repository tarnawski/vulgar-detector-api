{
  "name": "tarnawski/vulgar-detector",
  "authors": [
    {
      "name": "Tomasz Tarnawski",
      "email": "tarnawski27@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "symfony/symfony": "~3.0",
    "symfony/framework-bundle": "^3.0",
    "sensio/framework-extra-bundle": "^3.0",
    "friendsofsymfony/rest-bundle": "1.7.*",
    "incenteev/composer-parameter-handler": "~2.1",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/orm": "^2.5",
    "symfony/monolog-bundle": "~2.4",
    "nelmio/cors-bundle": "~1.4.0",
    "icecave/lace": "^0.1.0",
    "leaseweb/memcache-bundle": "*"
  },
  "require-dev": {
    "behat/behat": "~3.0",
    "behat/mink": "~1.6",
    "behat/mink-extension": "~2.0",
    "behat/symfony2-extension": "~2.0",
    "behat/mink-browserkit-driver": "~1.2",
    "bossa/phpspec2-expect": "*",
    "coduo/php-matcher": "2.0.1",
    "phpunit/phpunit": "~4.3",
    "phploc/phploc": "^3.0",
    "squizlabs/php_codesniffer": "~2.0",
    "phpmd/phpmd" : "@stable",
    "pdepend/pdepend" : "2.2.4",
    "sebastian/phpcpd": "^2.0",
    "codifico/parameter-bag-extension": "dev-master",
    "tarnawski/web-api-extension": "1.0.x@dev",
    "hautelook/alice-bundle": "^1.3",
    "doctrine/doctrine-fixtures-bundle": "^2.3"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
    ]
},
  "autoload": {
    "classmap": [
      "app/AppKernel.php"
    ],
    "psr-4": {
      "": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "incenteev-parameters": [
      {
        "file": "app/config/parameters.yml",
        "dist-file": "app/config/parameters.yml.dist",
        "env-map": {
          "secret_key": "SECRET_KEY"
        }
      }
    ]
  },
  "archive": {
    "exclude": [
      ".*",
      ".git",
      "vagrant/",
      "spec/",
      "features/"
    ]
  }
}
